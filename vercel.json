{
    "version": 2,
    "builds": [
      {
        "src": "personal_gallery/wsgi.py",
        "use": "@vercel/python"
      },
      {
          "src": "build.sh",
          "use": "@vercel/static-build",
          "config": {
              "distDir": "staticfiles_build"
          }
      }
    ],
    "routes": [
      {
          "src": "/static/(.*)",
          "dest": "/static/$1"
      },
      {
        "src": "/(.*)",
        "dest": "personal_gallery/wsgi.py"
      }
    ]
  }
  